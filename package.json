  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:3001": "next dev --turbopack --port 3001",
    "dev:3002": "next dev --turbopack --port 3002",
    "dev:4000": "next dev --turbopack --port 4000",
    "dev:cron": "node start-vercel-dev.js",
